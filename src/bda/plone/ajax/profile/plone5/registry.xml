<?xml version="1.0"?>
<registry xmlns:i18n="http://xml.zope.org/namespaces/i18n"
          i18n:domain="bda.plone.ajax">

<!-- Bundle resource. This is a file that will define a
     require method(requirejs) and be built -->
  <records prefix="plone.resources/bda-plone-ajax"
            interface='Products.CMFPlone.interfaces.IResourceRegistry'>
     <value key="css">
        <element>++resource++bdajax/bdajax.css</element>
     </value>
     <value key="js">++resource++bdajax/bdajax.js</value>
  </records>

  <records prefix="plone.bundles/bda-plone-ajax-bundle"
            interface='Products.CMFPlone.interfaces.IBundleRegistry'>
    <value key="resources">
      <element>bda-plone-ajax</element>
    </value>
    <value key="enabled">True</value>
    <value key="jscompilation">++resource++bdajax/bdajax.js</value>
    <value key="csscompilation">++resource++bdajax/bdajax.css</value>
    <value key="last_compilation">2016-02-22 20:30:00</value>
   </records>

</registry>
