<?xml version="1.0"?>
<registry xmlns:i18n="http://xml.zope.org/namespaces/i18n"
          i18n:domain="bda.plone.ajax">


  <records prefix="plone.resources/bdajax"
            interface='Products.CMFPlone.interfaces.IResourceRegistry'>
      <value key="js">++resource++bdajax/bdajax.js</value>
      <value key="export">window.bdajax</value>
      <value key="deps">jquery</value>
      <value key="css">
        <element>++resource++bdajax/bdajax.css</element>
      </value>
  </records>

  <records prefix="plone.resources/bdajax-overlay"
            interface='Products.CMFPlone.interfaces.IResourceRegistry'>
      <value key="js">++resource++bdajax/overlay.js</value>
      <value key="export">window.overlay</value>
      <value key="deps">jquery</value>
  </records>

