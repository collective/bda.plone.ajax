<?xml version="1.0"?>
<registry xmlns:i18n="http://xml.zope.org/namespaces/i18n"
          i18n:domain="bda.plone.ajax">

  <!--
    jquerytools overlay is included for resource bundle plone-logged-in only,
    so we provide it again for anonymous users.
  -->
  <records prefix="plone.bundles/bdajax-jquerytools-overlay"
           interface='Products.CMFPlone.interfaces.IBundleRegistry'>
    <value key="enabled">True</value>
    <value key="jscompilation">++resource++bdajax/overlay.js</value>
    <value key="compile">False</value>
    <value key="depends">plone</value>
    <value key="expression">python: member is None</value>
  </records>

  <records prefix="plone.bundles/bdajax"
           interface='Products.CMFPlone.interfaces.IBundleRegistry'>
    <value key="enabled">True</value>
    <value key="jscompilation">++resource++bdajax/bdajax.js</value>
    <value key="csscompilation">++resource++bdajax/bdajax.css</value>
    <value key="compile">False</value>
    <value key="depends">plone</value>
  </records>

</registry>
